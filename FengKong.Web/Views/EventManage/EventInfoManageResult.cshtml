
@{
    ViewData["Title"] = "EventInfoManageResult";
    Layout = null;
}
<style type="text/css">
</style>
@model List<FengKong.Models.EventInfoModel>

<table class="table table-striped table-bordered">
    <tr role="row">
        <th>app_id</th>
        <th>event_code</th>
        <th>event_name</th>
        <th>event_type</th>
        <th>操作</th>
    </tr>
    @foreach (var even in Model)
    {


        <tr>
            <td>@even.app_id</td>
            <td>@even.event_code</td>
            <td>@even.event_name</td>
            <td>@even.event_type</td>
            <td><a class="btn btn-success" href="/EventManage/EventInfoEdit?appid=@even.app_id&eventcode=@even.event_code">修改事件</a></td>
        </tr>

    }
</table>
